<UserControl x:Class="TimetableManager.WPF.StatisticsTimetableDataControls.SatatTimeViewControls.Tab_Main_stat"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:TimetableManager.WPF.StatisticsTimetableDataControls.StatisticUserControl"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <TabControl TabStripPlacement="Top"  Grid.Column="2" Grid.Row="2" Margin="10">
            <TabItem Header="Lecturer">
                <local:Tab_Stat_lecturer/>
            </TabItem>
            <TabItem Header="Student" >
                <local:Tab_Stat_Student/>
            </TabItem>
            <TabItem Header="Subject" >
                <local:Tab_Stat_subject/>
            </TabItem>

        </TabControl>
    </Grid>
</UserControl>
